{
  "name": "starter",
  "version": "0.0.1",
  "description": "Quasar template for quickly starting Lakeside R&D projects.",
  "productName": "Starter",
  "author": "Milos Paunovic <paun992@hotmail.com>",
  "private": true,
  "scripts": {
    "generator": "sh generator.sh",

    "lint": "eslint . --ext .js,.vue --fix",

    "dev:local": "cross-env ENVIRONMENT=local quasar dev",

    "prebuild:development": "npm run lint",
    "build:development": "cross-env ENVIRONMENT=development quasar build --debug",

    "prebuild:production": "npm run lint",
    "build:production": "cross-env ENVIRONMENT=production quasar build"
  },
  "dependencies": {
    "@quasar/extras": "^1.0.0",
    "@sentry/tracing": "^6.16.1",
    "@sentry/vue": "^6.16.1",
    "axios": "^0.21.1",
    "core-js": "^3.6.5",
    "dotenv": "^10.0.0",
    "quasar": "^1.0.0"
  },
  "devDependencies": {
    "@quasar/app": "^2.0.0",
    "babel-eslint": "^10.0.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.21.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-vue": "^7.7.0",
    "eslint-webpack-plugin": "^2.4.0"
  },
  "browserslist": [
    "last 10 Chrome versions",
    "last 10 Firefox versions",
    "last 4 Edge versions",
    "last 7 Safari versions",
    "last 8 Android versions",
    "last 8 ChromeAndroid versions",
    "last 8 FirefoxAndroid versions",
    "last 10 iOS versions",
    "last 5 Opera versions"
  ],
  "engines": {
    "node": ">= 10.18.1",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
